{
    "name": "robconvery/laravel-diary",
    "description": "Laravel package to scaffold a diary",
    "minimum-stability": "dev",
    "require": {},
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.7",
        "squizlabs/php_codesniffer": "^3.4",
        "fzaninotto/faker": "^1.9@dev"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Robert Convery",
            "email": "robcconvery@me.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Robconvery\\Laraveldiary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robconvery\\Laraveldiary\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robconvery\\Laraveldiary\\PackageServiceProvider"
            ]
        }
    },
    "scripts": {
        "cs": [
            "./vendor/bin/phpcs --standard=PSR2 src"
        ],
        "cs-fix": [
            "./vendor/bin/phpcbf --standard=PSR2 src"
        ]
    }
}
